<% errors_text = '' %>
<% errors.messages.each do |_, messages| %>
  <% messages.each do |msg| %>
    <% errors_text += "- #{msg} \n" %>
  <% end %>
<% end %>
<% errors_text = "#{t('errors.messages.not_saved', count: errors.count)} \r\n#{errors_text}" %>

AjaxErrorMessage.render("<%= j errors_text.html_safe %>");
